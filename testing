#include <stdio.h>

int main(void) {
    int arr[11];
    for (int i = 0; i < 11; ++i) {
        printf("Enter element %d: ", i);
        if (scanf("%d", &arr[i]) != 1) {
            fprintf(stderr, "Invalid input\n");
            return 1;
        }
    }

    printf("You entered:\n");
    for (int i = 0; i < 11; ++i)
        printf("%d%c", arr[i], (i==10)?'\n':' ');

    return 0;
}